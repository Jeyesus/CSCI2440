<script type = "text/javascript">
			
        function ShowEffect(element){
            new Effect.Appear(element, {duration:1, from:0, to:1.0});
        }
        function HideEffect(element){
            new Effect.Appear(element, {duration:0, from:1.0, to:0});
        }
		 
        function DropOutEffect(element){
            new Effect.DropOut(element, {duration:3});
        }
		 
		function ShrinkEffect(element){
            new Effect.Shrink(element, {duration:3});
        }
		 
		function FoldEffect(element){
            new Effect.Fold(element, {duration:3});
        }

		function myMove() {
			var elem = document.getElementById("myAnimation");   
			var pos = 0;
			var id = setInterval(frame, 10);
		function frame() {
			if (pos == 1000) {
			clearInterval(id);
			document.getElementById("myAnimation").style.visibility = "hidden";
			} 
			else if (pos < 300) {
			pos += 100; 
			elem.style.top = pos + 'px'; 
			}
			else if (pos == 300) {
			pos++; 
			new Effect.Appear(myAnimation, {duration:5, from:1.0, to:0});
			}
			else {
			document.getElementById("myAnimation").style.visibility = "visible";	
			pos++; 
			elem.style.left = pos + 'px'; 
    }
  }
}
		 
		 
		 
		 
</script>

</head>

